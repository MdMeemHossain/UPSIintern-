<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".StudentprofileActivity">

    <ImageView
        android:id="@+id/imageViewh1"
        android:layout_width="wrap_content"
        android:layout_height="83dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="40dp"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="-2dp"
        android:layout_marginEnd="23dp"
        android:layout_marginRight="23dp"
        android:layout_marginBottom="579dp"
        app:srcCompat="@drawable/titlelogo"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/nameformtv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="481dp"
        android:text="   Name"
        android:textSize="25dp" />

    <TextView
        android:id="@+id/studentidtv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="423dp"
        android:text="   Student ID"
        android:textSize="25dp" />

    <TextView
        android:id="@+id/facultytv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="373dp"
        android:text="   Faculty"
        android:textSize="25dp" />

    <TextView
        android:id="@+id/icpassportNotv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="323dp"
        android:text="   IC/Passport No."
        android:textSize="25dp" />

    <TextView
        android:id="@+id/emailcontactNotv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="271dp"
        android:text="   Email/Contact No."
        android:textSize="25dp" />

    <TextView
        android:id="@+id/permanentaddresstv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="211dp"
        android:text="   Permanent Address"
        android:textSize="25dp" />

    <TextView
        android:id="@+id/currentaddresstv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="159dp"
        android:text="   Current Address"
        android:textSize="25dp" />

    <Button
        android:id="@+id/editbutton"
        android:layout_width="350dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="23dp"
        android:layout_marginRight="23dp"
        android:layout_marginBottom="42dp"
        android:background="@color/purple_700"
        android:text="Edit"
        android:textColor="@color/black"
        android:textStyle="bold" />

</RelativeLayout>